<dialog id="shareDialog">
    Agenda : <span id="nomAgendaSD"></span><br>
    <input type="text" id="nameInputSD" class="form_control">
    <button class="btn" id="addPersonSD"><span class="bi bi-person-plus"></span></button><br>
    <div id="info_div"></div>
    <h3>Partages</h3>
    <table id="partagesSD">
        <tr>
            <th>Utilisateur</th>
            <th>Statut</th>
        </tr>
        <% for (const user of locals.users) { %>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.statut %></td>
            </tr>
        <% } %>
    </table>
    <button class="btn border" id="cancelSD">Annuler</button>
</dialog>